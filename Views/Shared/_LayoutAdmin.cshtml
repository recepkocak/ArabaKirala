<!DOCTYPE html>
<html lang="en">
<head>
    <!-- META SECTION -->
    <title>Atlant - Responsive Bootstrap Admin Template</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <!-- END META SECTION -->
    <!-- CSS INCLUDE -->
    <link rel="stylesheet" type="text/css" id="theme" href="~/ContentAdmin/css/theme-default.css" />
    <!-- EOF CSS INCLUDE -->
</head>
<body>
    <!-- START PAGE CONTAINER -->
    <div class="page-container">

        <!-- START PAGE SIDEBAR -->
        <div class="page-sidebar">
            <!-- START X-NAVIGATION -->
            @{ Html.RenderPartial("_menuAdmin");}
            <!-- END X-NAVIGATION -->
        </div>
        <!-- END PAGE SIDEBAR -->
        <!-- PAGE CONTENT -->
        <div class="page-content">

            @{ Html.RenderPartial("_navAdmin");}
            <!-- START BREADCRUMB -->
            <ul class="breadcrumb">
            </ul>
            <!-- END BREADCRUMB -->
            <!-- PAGE CONTENT WRAPPER -->
            @RenderBody()
            <!-- END PAGE CONTENT WRAPPER -->
        </div>
        <!-- END PAGE CONTENT -->
    </div>
    <!-- END PAGE CONTAINER -->
    <!-- MESSAGE BOX-->
    <div class="message-box animated fadeIn" id="mb-signout">
        <div class="mb-container">
            <div class="mb-middle">
                <div class="mb-title"><span class="fa fa-sign-out"></span> Çıkış <strong>Yap</strong> ?</div>
                <div class="mb-content">
                    <p>Çıkış yapmak istiyor musunuz?</p>
                    <p></p>
                </div>
                <div class="mb-footer">
                    <div class="pull-right">
                        <a href="pages-login.html" class="btn btn-success btn-lg">Evet</a>
                        <button class="btn btn-default btn-lg mb-control-close">Hayır</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- END MESSAGE BOX-->
    <!-- START SCRIPTS -->
    <!-- START PLUGINS -->
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/jquery/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/bootstrap/bootstrap.min.js"></script>
    <!-- END PLUGINS -->
    <!-- THIS PAGE PLUGINS -->
    <script type='text/javascript' src='~/ContentAdmin/js/plugins/icheck/icheck.min.js'></script>
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js"></script>

    <script type="text/javascript" src="~/ContentAdmin/js/plugins/bootstrap/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/bootstrap/bootstrap-file-input.js"></script>
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/bootstrap/bootstrap-select.js"></script>
    <script type="text/javascript" src="~/ContentAdmin/js/plugins/tagsinput/jquery.tagsinput.min.js"></script>
    <!-- END THIS PAGE PLUGINS -->
    <!-- START TEMPLATE -->

    <script type="text/javascript" src="~/ContentAdmin/js/plugins.js"></script>
    <script type="text/javascript" src="~/ContentAdmin/js/actions.js"></script>
    <!-- END TEMPLATE -->
    <!-- END SCRIPTS -->
</body>
</html>



