<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge"><![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>---------------------- @ViewBag.Title</title>

    <!-- Favicon -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/ico/apple-touch-icon-144-precomposed.png">
    <link rel="shortcut icon" href="~/Content/ico/favicon.ico">

    <!-- CSS Global -->
    <link href="~/Content/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/plugins/bootstrap-select/css/bootstrap-select.min.css" rel="stylesheet">
    <link href="~/Content/plugins/fontawesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/plugins/prettyphoto/css/prettyPhoto.css" rel="stylesheet">
    <link href="~/Content/plugins/owl-carousel2/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/plugins/owl-carousel2/assets/owl.theme.default.min.css" rel="stylesheet">
    <link href="~/Content/plugins/animate/animate.min.css" rel="stylesheet">
    <link href="~/Content/plugins/swiper/css/swiper.min.css" rel="stylesheet">
    <link href="~/Content/plugins/datetimepicker/css/bootstrap-datetimepicker.min.css" rel="stylesheet">
    <link href="~/Content/plugins/jquery-ui/jquery-ui.min.css" rel="stylesheet">
    <link href="~/Content/plugins/countdown/jquery.countdown.css" rel="stylesheet">
    <!-- Tema CSS -->
    <link href="~/Content/theme.css" rel="stylesheet">

    <!-- Head Libs -->
    <script src="~/Content/plugins/modernizr.custom.js"></script>

    <!--[if lt IE 9]>
    <script src="~/Content/plugins/iesupport/html5shiv.js"></script>
    <script src="~/Content/plugins/iesupport/respond.min.js"></script>
    <![endif]-->
</head>
<body id="home" class="wide">
    <!-- PRELOADER -->
    <div id="preloader">
        <div id="preloader-status">
            <div class="spinner">
                <div class="rect1"></div>
                <div class="rect2"></div>
                <div class="rect3"></div>
                <div class="rect4"></div>
                <div class="rect5"></div>
            </div>
            <div id="preloader-title">Yüklenİyor</div>
        </div>
    </div>
    <!-- /PRELOADER -->
    <!-- WRAPPER -->
    <div class="wrapper">

        <!-- HEADER -->
        <header class="header fixed">
            <div class="header-wrapper">
                <div class="container">

                    <!-- Logo -->
                    <div class="logo">
                        <a href="@Url.Action("Index","Home")"><img src="~/Content/img/logo-rentit.png" alt="Rent It" /></a>
                    </div>
                    <!-- /Logo -->
                    <!-- Mobile menu toggle button -->
                    <a href="#" class="menu-toggle btn ripple-effect btn-theme-transparent"><i class="fa fa-bars"></i></a>
                    <!-- /Mobile menu toggle button -->
                    <!-- Navigation -->
                    <nav class="navigation closed clearfix">
                        <div class="swiper-wrapper">
                            <div class="swiper-slide">
                                <!-- navigation menu -->
                                <a href="#" class="menu-toggle-close btn"><i class="fa fa-times"></i></a>
                                <ul class="nav sf-menu">
                                    <li class="active">
                                        <a href="@Url.Action("Index","Home")">Anasayfa</a>
                                    </li>
                                    <li class="active">
                                        <a href="@Url.Action("iletisim","Home")">İLETİŞİM</a>
                                    </li>
                                </ul>
                                <!-- /navigation menu -->
                            </div>
                        </div>
                        <!-- Add Scroll Bar -->
                        <div class="swiper-scrollbar"></div>
                    </nav>
                    <!-- /Navigation -->

                </div>
            </div>

        </header>
        <!-- /HEADER -->
        <!-- İçerik -->
        @RenderBody()
        <!-- /İçerik -->
        <!-- FOOTER -->
        @{ Html.RenderPartial("_footer");}
        <!-- /FOOTER -->

        <div id="to-top" class="to-top"><i class="fa fa-angle-up"></i></div>

    </div>
    <!-- /WRAPPER -->
    <!-- JS Global -->
    <script src="~/Content/plugins/jquery/jquery-1.11.1.min.js"></script>
    <script src="~/Content/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/plugins/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/Content/plugins/superfish/js/superfish.min.js"></script>
    <script src="~/Content/plugins/prettyphoto/js/jquery.prettyPhoto.js"></script>
    <script src="~/Content/plugins/owl-carousel2/owl.carousel.min.js"></script>
    <script src="~/Content/plugins/jquery.sticky.min.js"></script>
    <script src="~/Content/plugins/jquery.easing.min.js"></script>
    <script src="~/Content/plugins/jquery.smoothscroll.min.js"></script>
    <!--<script src="~/Content/plugins/smooth-scrollbar.min.js"></script>-->
    <!--<script src="~/Content/plugins/wow/wow.min.js"></script>-->
    <script>
    // WOW - animated content
    //new WOW().init();
    </script>
    <script src="~/Content/plugins/swiper/js/swiper.jquery.min.js"></script>
    <script src="~/Content/plugins/datetimepicker/js/moment-with-locales.min.js"></script>
    <script src="~/Content/plugins/datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
    <!-- JS Page Level -->
    <script src="~/Scripts/theme-ajax-mail.js"></script>
    <script src="~/Scripts/theme.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false"></script>
    <script src="~/Content/plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Content/plugins/isotope/jquery.isotope.min.js"></script>
</body>
</html>